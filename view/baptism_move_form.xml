<form cursor="group" col="6">
    <label name="number"/>
    <field name="number"/>
    <label name="post_number"/>
    <field name="post_number"/>
    <label name="company"/>
    <field name="company"/>
    <label name="date"/>
    <field name="date"/>
    <label name="post_date"/>
    <field name="post_date"/>
    <label name="group"/>
    <field name="group"/>
