<form col="4" cursor="meta">
    <label name="meta"/>
    <field name="meta" colspan="3"/>
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <label name="parent"/>
    <field name="parent"/>    
    <label name="active"/>
    <field name="active" xexpand="0" width="100"/>
    <notebook colspan="4">
        <page string="General Information" id="general">
            <label name="company"/>
            <field name="company"/>
            <label name="type"/>
            <field name="type"/>
        </page>
        <page string="Children" id="children">
            <field name="childs" colspan="4"/>
        </page>
        <page string="Other info" id="other">
            <label name="currency"/>
            <field name="currency"/>
        </page>

    </notebook>
</form>